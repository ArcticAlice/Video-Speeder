{
  "name": "Video Speeder",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Speeds & Slows Videos Beyond The Given Limit Of 0.25x - 2.0x",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popUp.html"
  },
  
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}
